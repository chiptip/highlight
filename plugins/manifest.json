{
  "name": "ouiba",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "chrome plugin for ouiba",
  "homepage_url": "http://ouiba.com",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "img/icon19.png",
    "default_title": "Click to scrape event",
    "default_popup": "popup.html"
  },
  "permissions": [ "http://*/*", "https://*/*", "tabs" ]
}
